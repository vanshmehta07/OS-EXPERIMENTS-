#include <stdio.h>
#include <stdlib.h>

#define FRAME_SIZE 3
#define PAGE_SIZE 10

int findLRU(int time[], int n) {
    int minimum = time[0], pos = 0;
    for (int i = 1; i < n; ++i) {
        if (time[i] < minimum) {
            minimum = time[i];
            pos = i;
        }
    }
    return pos;
}

void LRU(int pages[], int n) {
    int frame[FRAME_SIZE], time[FRAME_SIZE], i, j, pos, pageFaults = 0;
    for (i = 0; i < FRAME_SIZE; ++i) {
        frame[i] = -1;
    }
    
    for (i = 0; i < n; ++i) {
        int flag = 0;
        for (j = 0; j < FRAME_SIZE; ++j) {
            if (frame[j] == pages[i]) {
                flag = 1;
                break;
            }
        }
        
        if (flag == 0) {
            pos = findLRU(time, FRAME_SIZE);
            frame[pos] = pages[i];
            time[pos] = i;
            pageFaults++;
        } else {
            time[j] = i;
        }
        
        printf("Frame: ");
        for (j = 0; j < FRAME_SIZE; ++j) {
            if (frame[j] != -1) {
                printf("%d ", frame[j]);
            }
        }
        printf("\n");
    }
    printf("Total Page Faults: %d\n", pageFaults);
}

int main() {
    int pages[PAGE_SIZE] = {0, 1, 2, 0, 3, 0, 4, 2, 3, 0};
    int n = sizeof(pages) / sizeof(pages[0]);
    LRU(pages, n);
    return 0;
}
